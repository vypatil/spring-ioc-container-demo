<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">
 
 
 <bean  class="com.demo.BeanMethods.Student" name="std1" autowire="byName" >
 		
 		<property name="rollNo" value="1" />	
 		<property name="name" value="vaibhav" />	
</bean>

 <bean  class="com.demo.BeanMethods.Student" name="std2" autowire="byName" >
 		
 		<property name="rollNo" value="2" />	
 		<property name="name" value="shashank" />	
</bean>
 <bean  class="com.demo.BeanMethods.Student" name="std3" autowire="byName" >
 		
 		<property name="rollNo" value="3" />	
 		<property name="name" value="sush" />	
</bean>

<bean class="com.demo.BeanMethods.Address" name="address" >
	<property name="pin" value="41410"/>
	<property name="city" value="kolhapur"/>
</bean>
 
 
 
 
 
 </beans>